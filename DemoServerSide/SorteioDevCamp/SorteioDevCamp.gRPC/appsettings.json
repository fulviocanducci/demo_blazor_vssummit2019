{
  "ConnectionStrings": {
    "Default": "Server=tcp:{your_server},1433;Initial Catalog={database};Persist Security Info=False;User ID={your_username};Password={your_password};"
  }
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
